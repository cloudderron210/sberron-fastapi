services:
  db:
    image: postgres 
    restart: always
    environment:
      POSTGRES_USER: derron
      POSTGRES_PASSWORD: password
      POSTGRES_DB: debug2
    ports:
      - "15432:15432"
    command: >
      postgres -c logging-collector=on
               -c log-statement=all
               -c log-destination=stderr
      
      



      
